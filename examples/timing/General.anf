<analysis version="2">
    <inputs>
        <input pattern="results/General-*.vec"/>
        <input pattern="results/General-*.sca"/>
    </inputs>
    <charts>
        <chart icon="icons/full/obj/histogramchart.png" id="7" name="Server2, Queueing time, histograms (by name)" template="histogramchart_native" type="HISTOGRAM">
            <script>
                <![CDATA[from omnetpp.scave import results, chart, utils, plot

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]

# query vector data into a data frame
df = results.get_histograms(filter_expression, include_attrs=True, include_itervars=True)

if df.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# plot
utils.plot_histograms(df, props)
utils.postconfigure_plot(props)

utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]>
            </script>
