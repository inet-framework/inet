<?xml version="1.0"?>
<!DOCTYPE checker SYSTEM "../../../etc/MessageChecker.dtd">
<!-- checker xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="MessageChecker.xsd" -->
<checker>
    <message occurence="2"/> <!-- PassiveOPEN, ActiveOPEN -->
    <message occurence="1">
        <fieldValue name="className" value="inet::bgp::BGPOpenMessage"/>
    </message>
    <message occurence="1"/> <!-- ABORT -->
    <message occurence="1">
        <fieldValue name="className" value="inet::bgp::BGPKeepAliveMessage"/>
    </message>
    <message occurence="1">
        <fieldValue name="className" value="inet::bgp::BGPUpdate"/>
    </message>
</checker>
