<script>
    <at t="60s">
        <disconnect src-module="switch1" src-gate="ethg[1]" />
    </at>
    <at t="120s">
        <connect src-module="switch1" src-gate="ethg[1]"
                 dest-module="switch3" dest-gate="ethg[0]" 
                 channel-type="inet.node.ethernet.Eth1G">
        </connect>
    </at>
</script>