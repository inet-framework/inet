<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<analysis version="2">
    <inputs>
        <input pattern="results/*.vec"/>
        <input pattern="results/*.sca"/>
    </inputs>
    <charts>
        <chart createdWith="OMNeT++ 7.0.0.221114-493122b604" icon="icons/full/obj/barchart.png" id="1278166528" name="Summary Linechart" resultTypes="scalar" template="barchart_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

def make_chart(filter_expression):
    # collect parameters for query
    include_fields = props["include_fields"] == "true"
    
    # query scalar data into dataframe
    try:
        df = results.get_scalars(filter_expression, include_fields=include_fields, include_attrs=True, include_runattrs=True, include_itervars=True)
    except results.ResultQueryError as e:
        raise chart.ChartScriptError("Error while querying results: " + str(e))
    
    if df.empty:
        raise chart.ChartScriptError("The result filter returned no data.")
    
    groups, series = utils.select_groups_series(df, props)
    confidence_level = utils.get_confidence_level(props)
    valuedf, errorsdf, metadf = utils.pivot_for_barchart(df, groups, series, confidence_level)
    
    (100 - valuedf.T).plot.line(ax=plt.gca())
    plt.ylabel("ping success %")

import matplotlib.pyplot as plt
plt.gcf().subplots(3, 3)

for x, radio in enumerate(["Generic", "Apsk", "Ieee80211"]):
    for y, medium in enumerate(["UnitDisk", "Scalar", "Dimensional"]):
        filter = f'type =~ scalar AND isfield =~ false AND module =~ "*.host[1].*" AND name =~ "ping loss rate (%)" AND runattr:configname =~ *{radio}*{medium}*'
        plt.subplot(3, 3, 3*y+x+1)
        utils._initialize_cycles(props)
        make_chart(filter)
        plt.gca().set_title(radio + " " + medium)


utils.postconfigure_plot(props)

utils.export_image_if_needed(props)
]]></script>
