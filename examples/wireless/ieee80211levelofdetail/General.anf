        <chart createdWith="" icon="icons/full/obj/scatterchart.png" id="1941276697" name="count vs SNIR" resultTypes="" template="scatterchart_itervars_mpl" type="MATPLOTLIB">
            <script><![CDATA[import math
import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
from omnetpp.scave import results, chart, utils, ideplot, vectorops as ops

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
xaxis_itervar = props["xaxis_itervar"]
iso_itervar = props["iso_itervar"]

pd.set_option('display.max_rows', None)  # or 1000

# query data into a data frame
df = results.get_scalars(filter_expression, include_runattrs=True, include_attrs=True, include_itervars=True)
df = df[["name", "configname", "bitrate", "distance", "value"]]
df2 = results.get_histograms("name =~ minSnir:histogram AND module =~ *destination*", include_itervars=True, include_runattrs=True)
df2["SNIR"] = np.log10(df2["mean"]) * 10
df2 = df2[["configname", "bitrate", "distance", "SNIR"]]
df = df.merge(df2, on=["configname", "bitrate", "distance"])
df["isoline"] = df["configname"] + "-" + df["bitrate"]

#v1 = sorted(df[df["configname"] == "Default"]["SNIR"].unique())
#v2 = sorted(df[df["configname"] == "Layered"]["SNIR"].unique())
#print(v1)
#print(v2)

def plotit(df):
    df[xaxis_itervar] = pd.to_numeric(df[xaxis_itervar], errors="ignore")
    df[iso_itervar] = pd.to_numeric(df[iso_itervar], errors="ignore")

    if df.empty:
        raise chart.ChartScriptError("Select scalars for the Y axis in the Properties dialog")

    unique_names = df["name"].unique()

    if len(unique_names) != 1:
        raise chart.ChartScriptError("Selected scalars must share the same name.")

    scalar_name = unique_names[0]

    confidence_level_str = props["confidence_level"] if "confidence_level" in props else "none"

    if confidence_level_str == "none":
        df = pd.pivot_table(df, values="value", columns="isoline", index="SNIR")
        errors_df = None
    else:
        confidence_level = float(confidence_level_str[:-1])/100
        conf_int = lambda values: utils.confidence_interval(confidence_level, values) if len(values) > 1 else math.nan
        pivoted = pd.pivot_table(df, values="value", columns=iso_itervar, index=xaxis_itervar, aggfunc=[np.mean, conf_int], dropna=False)
        df = pivoted["mean"]
        errors_df = pivoted["<lambda>"]

    legend_cols, _ = utils.extract_label_columns(df, props)

    p = plot if chart.is_native_chart() else plt

    p.xlabel(xaxis_itervar)
    p.ylabel(scalar_name)

    try:
        xs = pd.to_numeric(df.index.values)
    except:
        raise chart.ChartScriptError("The X axis itervar is not purely numeric, this is not supported yet.")

    for c in df.columns:
        style = utils._make_line_args(props, c, df)
        style["linestyle"] = ":" if c.startswith("Layered") else "-"
        style["color"] = "C" + str(int(int(c.split("-")[1]) / 3))
        ys = df[c].values
        p.plot(xs, ys, label=iso_itervar + "=" + str(df[c].name), **style)

        if errors_df is not None and not chart.is_native_chart():
            style["linewidth"] = float(style["linewidth"])
            style["linestyle"] = "none"
            yerr = errors_df[c].values
            if props["error_style"] == "Error bars":
                p.errorbar(xs, ys, yerr=yerr, capsize=float(props["cap_size"]), **style)
            elif props["error_style"] == "Error band":
                plt.fill_between(xs, ys-yerr, ys+yerr, alpha=float(props["band_alpha"]))

    utils.set_plot_title(scalar_name + " vs. " + xaxis_itervar)

plotit(df[df["configname"] == "Default"])
plotit(df[df["configname"] == "Layered"])

utils.postconfigure_plot(props)

utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]></script>
