#  ** This file was automatically generated by the command:
#  opp_makemake -f -N -b /home/andras/INET -c /home/andras/INET/inetconfig -n -r -I../../Network/IPv4 -I../../Transport/TCP -I../../Base -I../../Util
#

# Name of target to be created (-o option)
TARGET = TCPApp

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../../Network/IPv4 -I../../Transport/TCP -I../../Base -I../../Util

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with (wildcard needed to prevent "no such file *.o" errors)
EXT_DIR_OBJS=

# time stamps of other directories (used as dependency)
EXT_DIR_TSTAMPS= ../../Network/IPv4/.tstamp ../../Transport/TCP/.tstamp ../../Base/.tstamp ../../Util/.tstamp

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../../inetconfig
include ../../inetconfig

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS= GenericAppMsg_m.o  TCPBasicClientApp.o TCPEchoApp.o TCPGenericCliAppBase.o TCPGenericSrvApp.o TCPGenericSrvThread.o TCPSessionApp.o TCPSinkApp.o TCPSrvHostApp.o TelnetApp.o

# header files generated (from msg files)
GENERATEDHEADERS= GenericAppMsg_m.h

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) subdirs Makefile .tstamp
	@# do nothing

.tstamp: $(OBJS)
	echo>.tstamp

$(OBJS) : $(GENERATEDHEADERS)


purify: $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_TSTAMPS) subdirs Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

$(EXT_DIR_TSTAMPS):
	@echo
	@echo Error: $@ does not exist.
	@echo This means that at least the above dependency directory has not been built.
	@echo Maybe you need to do a top-level make?
	@echo
	@exit 1

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

GenericAppMsg_m.o: GenericAppMsg_m.cc
	$(CXX) -c $(NEDCOPTS) GenericAppMsg_m.cc

GenericAppMsg_m.cc GenericAppMsg_m.h: GenericAppMsg.msg
	$(MSGC) $(INCLUDE_PATH) GenericAppMsg.msg

TCPBasicClientApp.o: TCPBasicClientApp.cc
	$(CXX) -c $(COPTS) TCPBasicClientApp.cc

TCPEchoApp.o: TCPEchoApp.cc
	$(CXX) -c $(COPTS) TCPEchoApp.cc

TCPGenericCliAppBase.o: TCPGenericCliAppBase.cc
	$(CXX) -c $(COPTS) TCPGenericCliAppBase.cc

TCPGenericSrvApp.o: TCPGenericSrvApp.cc
	$(CXX) -c $(COPTS) TCPGenericSrvApp.cc

TCPGenericSrvThread.o: TCPGenericSrvThread.cc
	$(CXX) -c $(COPTS) TCPGenericSrvThread.cc

TCPSessionApp.o: TCPSessionApp.cc
	$(CXX) -c $(COPTS) TCPSessionApp.cc

TCPSinkApp.o: TCPSinkApp.cc
	$(CXX) -c $(COPTS) TCPSinkApp.cc

TCPSrvHostApp.o: TCPSrvHostApp.cc
	$(CXX) -c $(COPTS) TCPSrvHostApp.cc

TelnetApp.o: TelnetApp.cc
	$(CXX) -c $(COPTS) TelnetApp.cc


#doc: neddoc doxy

#neddoc:
#	opp_neddoc -a

#doxy: doxy.cfg
#	doxygen doxy.cfg

generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -N -b /home/andras/INET -c /home/andras/INET/inetconfig -n -r -I../../Network/IPv4 -I../../Transport/TCP -I../../Base -I../../Util 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefiles) || exit 1; done

makefile-ins:
	# recreate Makefile.in
	opp_makemake -f -m  -N -b /home/andras/INET -c /home/andras/INET/inetconfig -n -r -I../../Network/IPv4 -I../../Transport/TCP -I../../Base -I../../Util 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

# "re-makemake" and "re-makemake-m" are deprecated, historic names of the above two targets
re-makemake: makefiles
re-makemake-m: makefile-ins


