#
#  Makefile for TCP
#
#  ** This file was automatically generated by the command:
#  opp_makemake -f -N -b /home/andras/INET -c /home/andras/INET/inetconfig -n -I../../Network/IPv4 -I../../Base -I../../Util
#

# Name of target to be created (-o option)
TARGET = TCP

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../../Network/IPv4 -I../../Base -I../../Util

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with (wildcard needed to prevent "no such file *.o" errors)
EXT_DIR_OBJS=

# time stamps of other directories (used as dependency)
EXT_DIR_TSTAMPS= ../../Network/IPv4/.tstamp ../../Base/.tstamp ../../Util/.tstamp

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../../inetconfig
include ../../inetconfig

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS= TCPCommand_m.o TCPSegment_m.o DummyTCPAlg.o TCPConnectionBase.o TCPConnectionEventProc.o TCPConnectionRcvSegment.o TCPConnectionUtil.o TCPDump.o TCPMain.o TCPMsgBasedRcvQueue.o TCPMsgBasedSendQueue.o TCPSegment.o TCPSocket.o TCPSocketMap.o TCPTahoeReno.o TCPVirtualDataRcvQueue.o TCPVirtualDataSendQueue.o

# header files generated (from msg files)
GENERATEDHEADERS= TCPCommand_m.h TCPSegment_m.h

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) Makefile .tstamp
	@# do nothing

.tstamp: $(OBJS)
	echo>.tstamp

$(OBJS) : $(GENERATEDHEADERS)


purify: $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_TSTAMPS) Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

$(EXT_DIR_TSTAMPS):
	@echo
	@echo Error: $@ does not exist.
	@echo This means that at least the above dependency directory has not been built.
	@echo Maybe you need to do a top-level make?
	@echo
	@exit 1

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

TCPCommand_m.o: TCPCommand_m.cc
	$(CXX) -c $(NEDCOPTS) TCPCommand_m.cc

TCPCommand_m.cc TCPCommand_m.h: TCPCommand.msg
	$(MSGC) $(INCLUDE_PATH) TCPCommand.msg

TCPSegment_m.o: TCPSegment_m.cc
	$(CXX) -c $(NEDCOPTS) TCPSegment_m.cc

TCPSegment_m.cc TCPSegment_m.h: TCPSegment.msg
	$(MSGC) $(INCLUDE_PATH) TCPSegment.msg

DummyTCPAlg.o: DummyTCPAlg.cc
	$(CXX) -c $(COPTS) DummyTCPAlg.cc

TCPConnectionBase.o: TCPConnectionBase.cc
	$(CXX) -c $(COPTS) TCPConnectionBase.cc

TCPConnectionEventProc.o: TCPConnectionEventProc.cc
	$(CXX) -c $(COPTS) TCPConnectionEventProc.cc

TCPConnectionRcvSegment.o: TCPConnectionRcvSegment.cc
	$(CXX) -c $(COPTS) TCPConnectionRcvSegment.cc

TCPConnectionUtil.o: TCPConnectionUtil.cc
	$(CXX) -c $(COPTS) TCPConnectionUtil.cc

TCPDump.o: TCPDump.cc
	$(CXX) -c $(COPTS) TCPDump.cc

TCPMain.o: TCPMain.cc
	$(CXX) -c $(COPTS) TCPMain.cc

