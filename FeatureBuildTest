	cd src && $(MAKEMAKE) -f --deep --make-so -o inet -O out -pINET -DWITH_TCP_COMMON -DWITH_TCP_INET -DWITH_TCP_LWIP -DWITH_TCP_NSC -DWITH_IPv4 -DWITH_IPv6 -DWITH_xMIPv6 -DWITH_UDP -DWITH_RTP -DWITH_SCTP -DWITH_NETPERFMETER -DWITH_DHCP -DWITH_ETHERNET -DWITH_PPP -DWITH_EXT_IF -DWITH_MPLS -DWITH_OSPFv2 -DWITH_BGPv4 -DWITH_RIP -DWITH_TRACI -DWITH_RADIO -DWITH_AODV -DWITH_MANET -DWITH_IEEE80211 -DWITH_VOIPSTREAM
	$(RUNTEST)

