        <chart createdWith="OMNeT++ 7.0.0.230526-85b71cbd8c" icon="icons/full/obj/linechart.png" id="1511022258" name="Data rate and tokens - traffic category 1" resultTypes="vector" template="linechart_separate_mpl" type="MATPLOTLIB">
            <script><![CDATA[import math
import matplotlib.pyplot as plt
from omnetpp.scave import results, chart, utils
import os
import sys
# TODO workaround when using omnetpp 6.0 that doesn't yet contain the python path feature
sys.path.append(os.path.join(os.getcwd(), "../../../../python"))
import matplotlib.pyplot as plt
import inet.scave.plot
default_colors = inet.scave.plot.default_colors

traffic_category_index = 1
stream_index = traffic_category_index - 1

# get chart properties
props = chart.get_properties()

utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
start_time = float(props["vector_start_time"] or -math.inf)
end_time = float(props["vector_end_time"] or math.inf)

fe_datarate = """type =~ vector AND runattr:experiment =~ General AND runattr:replication =~ "#0" AND module =~ "PeekingUnderTheHoodShowcase.filter.filter[""" + str(stream_index) + """]" AND name =~ outgoingDataRate:vector"""
fe_tokens = """type =~ vector AND runattr:experiment =~ General AND runattr:replication =~ "#0" AND module =~ "PeekingUnderTheHoodShowcase.filter.meter[""" + str(stream_index) + """]" AND name =~ numTokens:vector"""

# query vector data into a data frame
try:
    df_datarate = results.get_vectors(fe_datarate, include_attrs=True, include_runattrs=True, include_itervars=True, start_time=start_time, end_time=end_time)
    df_tokens = results.get_vectors(fe_tokens, include_attrs=True, include_runattrs=True, include_itervars=True, start_time=start_time, end_time=end_time)
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df_datarate.empty or df_tokens.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# apply vector operations
df_datarate = utils.perform_vector_ops(df_datarate, props["vector_operations"])

print(df_datarate)

# plot
ax_datarate, ax_tokens = inet.scave.plot.plot_vectors_separate_grouped([df_datarate, df_tokens], props)

utils.postconfigure_plot(props)

plt.suptitle("Outgoing Data Rate and Number of Tokens - Traffic Category " + str(traffic_category_index))
ax_datarate.set_ylabel("Data rate [Mbps]")
ax_tokens.set_xlabel("Simulation time [s]")

for i in [ax_datarate, ax_tokens]:
    i.set_ylim(i.get_ylim()[0], i.get_ylim()[1]*1.15)

plt.subplots_adjust(left = 0.1, right = 0.99, top = 0.94, bottom = 0.1, wspace=0.1, hspace=0.1)

props['image_export_filename'] += str(traffic_category_index)

utils.export_image_if_needed(props)
#utils.export_data_if_needed(df, props)]]></script>
