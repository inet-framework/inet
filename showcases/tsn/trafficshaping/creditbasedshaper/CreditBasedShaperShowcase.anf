<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<analysis version="2">
    <inputs>
        <input pattern="results/General-#0.sca"/>
        <input pattern="results/General-#0.vec"/>
    </inputs>
    <charts>
        <chart icon="icons/full/obj/linechart.png" id="334386820" name="Client Application Traffic and Shaper Incoming Traffic" template="linechart_mpl" type="MATPLOTLIB">
            <script><![CDATA[import math
from omnetpp.scave import results, chart, utils, ideplot
import matplotlib.pyplot as plt
import os
import sys
# TODO workaround when using omnetpp 6.0 that doesn't yet contain the python path feature
sys.path.append(os.path.join(os.getcwd(), "../../../../python"))
import matplotlib.pyplot as plt
import inet.scave.plot
default_colors = inet.scave.plot.default_colors

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
start_time = float(props["vector_start_time"] or -math.inf)
end_time = float(props["vector_end_time"] or math.inf)

# query vector data into a data frame
try:
    df = results.get_vectors(filter_expression, include_attrs=True, include_itervars=True, start_time=start_time, end_time=end_time)
except ValueError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# apply vector operations
df = utils.perform_vector_ops(df, props["vector_operations"])

style_tuple_list = [('module','TsnLinearNetwork.client.app\\[0\\].source', {'color': default_colors[0]}),
                    ('module','TsnLinearNetwork.client.app\\[1\\].source', {'color': default_colors[1]}), 
                    ('module','TsnLinearNetwork.switch.eth\\[1\\].macLayer.queue.queue\\[0\\]', {'color': default_colors[0]}),
                    ('module','TsnLinearNetwork.switch.eth\\[1\\].macLayer.queue.queue\\[1\\]', {'color': default_colors[1]}),
                    ('source','throughput\\(packetPushed\\)', {'linestyle': 'dashed'}),
                    ('source','throughput\\(packetPushStarted\\)', {'linestyle': 'solid'})]

inet.scave.plot.add_to_dataframe(df, style_tuple_list)

# plot
inet.scave.plot.plot_vectors(df, props)

utils.postconfigure_plot(props)

legend_style = [('client application', {'linestyle': 'dashed', 'color': 'black'}),
                ('shaper incoming', {'linestyle': '-', 'color': 'black'}),
                ('best effort', {'color': default_colors[0], 'marker': 's', 'markersize': 4, 'linestyle': ''}),
                ('video', {'color': default_colors[1], 'marker': 's', 'markersize': 4, 'linestyle': ''})]

handles, labels = inet.scave.plot.create_custom_legend(props, style_tuple_list=legend_style)

plt.legend(handles, labels, ncol=2)

plt.tight_layout()

utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]></script>
