            <property name="filter" value="type =~ vector AND module =~ &quot;TsnLinearNetwork.switch.eth[1].macLayer.queue.queue[*]&quot; AND name =~ &quot;*DataRate:vector&quot;"/>
            <property name="vector_start_time" value=""/>
