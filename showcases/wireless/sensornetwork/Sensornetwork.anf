        <chart createdWith="" icon="icons/full/obj/barchart.png" id="331077454" name="Power consumption per packet" resultTypes="" template="barchart_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils
import best_performance
import matplotlib.pyplot as plt
import os
import sys
# TODO workaround when using omnetpp 6.0 that doesn't yet contain the python path feature
sys.path.append(os.path.join(os.getcwd(), "../../../python"))
import inet.scave.plot
default_colors = inet.scave.plot.default_colors

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
include_fields = props["include_fields"] == "true"

# get residualEnergyCapacity values summed for all modules in the network in dataframes
bmac_best_power_sum, xmac_best_power_sum, lmac_best_power_sum = best_performance.get_best_power_dataframes_for_most_packets_received()

# get residualEnergyCapacity value from dataframes, and invert to get power consumption
# and convert to mJ
bmac_power = bmac_best_power_sum['value'].iloc[0] * -1000
xmac_power = xmac_best_power_sum['value'].iloc[0] * -1000
lmac_power = lmac_best_power_sum['value'].iloc[0] * -1000

# get the runs with the most received packets
bmac_best_packets, xmac_best_packets, lmac_best_packets = best_performance.get_best_performing_dataframe_separately_packetreceived()

# get value of received packets from dataframes
bmac_packets = bmac_best_packets['value'].iloc[0]
xmac_packets = xmac_best_packets['value'].iloc[0]
lmac_packets = lmac_best_packets['value'].iloc[0]

# calculate power consumption per packet
bmac = bmac_power / bmac_packets
xmac = xmac_power / xmac_packets
lmac = lmac_power / lmac_packets

# collect plot data
x = ['BMac', 'XMac', 'LMac']
y = [bmac, xmac, lmac]

# plot
plt.bar(x,y, zorder=2)

plt.title("Power Consumption Per Packet")

utils.postconfigure_plot(props)

plt.grid(axis='y', linestyle='--')

plt.xlabel("")
plt.ylabel("")

ax = plt.gca()
#for p in ax.patches:
#    if p.get_height() < 5:
#       color = 'black'
#       offset = -4
#    else:
#       color = 'white'
#       offset = 2.5
#    ax.annotate(str(round(p.get_height(),2)) + ' mJ', (p.get_x() + p.get_width() / 2, p.get_height() - offset),
#                horizontalalignment='center', verticalalignment='top', color=color, size=10, zorder=12)

inet.scave.plot.annotate_barchart(ax, offset=0.5, size=10, postfix=' mJ')

utils.export_image_if_needed(props)
#utils.export_data_if_needed(df, props)
]]></script>
