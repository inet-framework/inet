            <script><![CDATA[import math
from omnetpp.scave import results, chart, utils
import os
import sys
# TODO workaround when using omnetpp 6.0 that doesn't yet contain the python path feature
sys.path.append(os.path.join(os.getcwd(), "../../../python"))
import matplotlib.pyplot as plt
import inet.scave.plot
default_colors = inet.scave.plot.default_colors
import solid_x_axis as sxa

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
start_time = float(props["vector_start_time"] or -math.inf)
end_time = float(props["vector_end_time"] or math.inf)
import os
import sys
# TODO workaround when using omnetpp 6.0 that doesn't yet contain the python path feature
sys.path.append(os.path.join(os.getcwd(), "../../../../python"))
import matplotlib.pyplot as plt
import inet.scave.plot
default_colors = inet.scave.plot.default_colors
filter_string_nonqos = """type =~ vector AND name =~ endToEndDelay:vector
AND NOT (type =~ vector AND runattr:experiment =~ Qos AND runattr:replication =~ "#0" AND name =~ endToEndDelay:vector AND (
    (module =~ "QosShowcase.server.app[2]") OR
    (module =~ "QosShowcase.server.app[3]") OR
    (module =~ "QosShowcase.server.app[1]") OR
    (module =~ "QosShowcase.server.app[0]")
))"""

filter_string_qos = """type =~ vector AND name =~ endToEndDelay:vector
AND NOT (type =~ vector AND runattr:experiment =~ NonQos AND runattr:replication =~ "#0" AND name =~ endToEndDelay:vector AND (
    (module =~ "QosShowcase.server.app[0]") OR
    (module =~ "QosShowcase.server.app[1]") OR
    (module =~ "QosShowcase.server.app[2]") OR
    (module =~ "QosShowcase.server.app[3]")
))"""

# query vector data into a data frame
try:
    df_nonqos = results.get_vectors(filter_string_nonqos, include_attrs=True, include_runattrs=True, include_itervars=True, start_time=start_time, end_time=end_time)
    df_qos = results.get_vectors(filter_string_qos, include_attrs=True, include_runattrs=True, include_itervars=True, start_time=start_time, end_time=end_time)
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df_nonqos.empty or df_qos.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# apply vector operations
df_nonqos = utils.perform_vector_ops(df_nonqos, props["vector_operations"])
df_qos = utils.perform_vector_ops(df_qos, props["vector_operations"])

inet.scave.plot.add_to_dataframe(df_nonqos, style_tuple_list=[('module', '.*0.*', {'color': default_colors[0]}),
                                                           ('module', '.*1.*', {'color': default_colors[1]}),
                                                           ('module', '.*2.*', {'color': default_colors[2]}),
                                                           ('module', '.*3.*', {'color': default_colors[3]})])

inet.scave.plot.add_to_dataframe(df_qos, style_tuple_list=[('module', '.*0.*', {'color': default_colors[0]}),
                                                        ('module', '.*1.*', {'color': default_colors[1]}),
                                                        ('module', '.*2.*', {'color': default_colors[2]}),
                                                        ('module', '.*3.*', {'color': default_colors[3]})])

df = [df_nonqos, df_qos]

# plot
ax1, ax2 = inet.scave.plot.plot_vectors_separate_grouped(df, props, layout='horizontal')

utils.postconfigure_plot(props)

inet.scave.plot.fix_labels_for_subplots([ax1, ax2], props, 'horizontal', remove_ticks=False)

ax1.legend(loc=1, ncol=4, handletextpad=0.1, columnspacing=0.1, markerscale=3)
ax2.legend(loc=1, ncol=4, handletextpad=0.1, columnspacing=0.1, markerscale=3)
ax1.grid(linestyle='--')
ax2.grid(linestyle='--')

ax1.title.set_text("Non-Qos")
ax2.title.set_text("Qos")

plt.suptitle('Instantaneous packet delay variation',y=1, size=14)

ax = plt.gca()

a = ax.get_ygridlines()

# get default grid color
ygridlines = ax1.get_ygridlines()
default_grid_color = ygridlines[0].get_color()

def on_ylims_change(axes):
    sxa.solid_x_axis(ax1,default_grid_color)
    sxa.solid_x_axis(ax2,default_grid_color)
    
ax1.callbacks.connect('ylim_changed', on_ylims_change)
ax2.callbacks.connect('ylim_changed', on_ylims_change)

on_ylims_change(ax1)
on_ylims_change(ax2)


utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]></script>
