        <chart createdWith="OMNeT++ 7.0.0.220921-6e93d9d2c9" icon="icons/full/obj/scatterchart.png" id="557977466" name="Throughput vs. SNIR in the case of 2000-byte packet size and 250-byte fragmentation threshold" resultTypes="scalar" template="scatterchart_itervars_mpl" type="MATPLOTLIB">
            <script><![CDATA[from omnetpp.scave import results, chart, utils, ideplot
import os
import sys
from omnetpp.scave.utils import make_legend_label
# TODO workaround when using omnetpp 6.0 that doesn't yet contain the python path feature
sys.path.append(os.path.join(os.getcwd(), "../../../python"))
import matplotlib.pyplot as plt
import modifiedplot
default_colors = modifiedplot.default_colors

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
include_fields = props["include_fields"] == "true"

filter_expression_snir = """(module =~ "FragmentationShowcase.wifiHost2.wlan[0].radio" AND name =~ minSnir:histogram:mean) AND
    (runattr:experiment =~ TxPower*)"""
    
filter_expression_tp = """(module =~ "FragmentationShowcase.wifiHost2.app[0]" AND name =~ throughput:vector:mean) AND
    (runattr:experiment =~ TxPower*)"""

# query data into a data frame
try:
    df_snir = results.get_scalars(filter_expression, include_fields=include_fields, include_attrs=True, include_runattrs=True, include_itervars=True)
    df_tp = results.get_scalars(filter_expression_tp, include_fields=include_fields, include_attrs=True, include_runattrs=True, include_itervars=True)
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df_snir.empty or df_tp.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

xaxis_itervar, group_by = utils.select_xaxis_and_groupby(df_snir, props)
confidence_level = utils.get_confidence_level(props)
newdf = utils.pivot_for_scatterchart(df_snir, xaxis_itervar, group_by, confidence_level)
newdf_tp = utils.pivot_for_scatterchart(df_tp, xaxis_itervar, group_by, confidence_level)

newdf.x = newdf.y
newdf.y = newdf_tp.y / 1000000

modifiedplot.add_to_dataframe(newdf, style_tuple_list=[('name', '.*DCFnofrag', {'color': default_colors[2]}),
                                                       ('name', '.*DCFfrag', {'color': default_colors[3]}),
                                                       ('name', '.*HCFfrag', {'color': default_colors[0]}),
                                                       ('name', '.*HCFfragblockack', {'color': default_colors[1]})], 
                                                       order=['name', '.*DCFnofrag', '.*DCFfrag', '.*HCFfrag', '.*HCFfragblockack'], 
                                                       default_dict={'marker': 's', 'markersize': 3})

props['legend_replacements'] = "/experiment=TxPower//"
props['legend_format'] = "${name}"

modifiedplot.plot_lines(newdf, props, debug=False)

names = df_snir["name"].unique()
scalar_names = names[0] + ", etc." if len(names) > 1 else names[0]
ideplot.ylabel(scalar_names) # TODO: shouldn't call into ideplot directly

title = (scalar_names + " vs. " + xaxis_itervar) if xaxis_itervar else scalar_names
if "title" in props and props["title"]:
    title = props["title"]
utils.set_plot_title(title)

utils.postconfigure_plot(props)

plt.grid(linestyle='--')
plt.legend(ncol=4, markerscale=2)
plt.tight_layout(pad=0)

utils.export_image_if_needed(props)
#utils.export_data_if_needed(df, props)
]]></script>
