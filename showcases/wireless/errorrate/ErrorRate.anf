        <chart createdWith="" icon="icons/full/obj/linechart.png" id="962028107" name="PER vs SNIR" resultTypes="" template="linechart_mpl" type="MATPLOTLIB">
            <script><![CDATA[import math
from omnetpp.scave import results, chart, utils
import matplotlib.pyplot as plt
import pandas as pd

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
start_time = float(props["vector_start_time"] or -math.inf)
end_time = float(props["vector_end_time"] or math.inf)

filter_expression_per = """module =~ "ErrorRateShowcase.destinationHost.wlan[*].radio" AND (name =~ "packetErrorRate:vector:mean")"""
filter_expression_snir = """module =~ "ErrorRateShowcase.destinationHost.wlan[*].radio" AND (name =~ "minSnir:vector:mean")"""

# query vector data into a data frame
try:
    df_per = results.get_scalars(filter_expression_per, include_fields = True, include_attrs=True, include_itervars=True, include_runattrs=True)
    df_snir = results.get_scalars(filter_expression_snir, include_fields = True, include_attrs=True, include_itervars=True, include_runattrs=True)
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df_per.empty or df_snir.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

df_per['per'] = df_per['value']             # add per column, same as value column
df_per = df_per[['iterationvarsd','per']]   # we only need these two columns

df_snir['snir'] = df_snir['value']          # add snir column, same as value column

df_joined = df_snir.merge(df_per, on='iterationvarsd')

df_joined['snir'] = pd.to_numeric(df_joined['snir'])
df_joined['per'] = pd.to_numeric(df_joined['per'])

df_joined = df_joined.pivot_table(columns='bitrate', index='snir', values='per')

df_joined = df_joined[['6','9','12','18','24','36','48','54']]

# apply vector operations
#df = utils.perform_vector_ops(df, props["vector_operations"])

# plot
for c in df_joined:
    ys = df_joined[c]
    mask = ys.notna()
    plt.plot(df_joined.index.values[mask], df_joined[c].values[mask], label=c + ' Mbps')

ylocs, ylabels = plt.yticks()

# TODO these should be set from the dialog 
# -> cannot do that because we're not using the built-in function to plot
plt.title('PER vs SNIR')
plt.xlabel('Signal-to-noise-plus-interference ratio')
plt.ylabel('Packet error rate')

utils.postconfigure_plot(props)

utils.export_image_if_needed(props)
#utils.export_data_if_needed(df, props)
]]></script>
