	    IPv6Header -> Ipv6Header
	    MACAddress -> MacAddress
	    MACAddressType -> MacAddressType
	    RandomQoSClassifier -> RandomQosClassifier
	    SAPCode -> SapCode
	    TCPSegment -> TcpHeader

