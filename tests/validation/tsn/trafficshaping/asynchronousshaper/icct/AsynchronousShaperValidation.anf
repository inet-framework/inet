<analysis version="2">
    <inputs>
        <input pattern="/inet/tests/validation/tsn/trafficshaping/asynchronousshaper/icct/results/*.vec"/>
        <input pattern="/inet/tests/validation/tsn/trafficshaping/asynchronousshaper/icct/results/*.sca"/>
