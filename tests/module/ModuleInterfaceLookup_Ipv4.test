%description:

Check module interface lookup of Ipv4 module.

%file: TestNetwork.ned

import inet.common.MessageDispatcher;
import inet.queueing.sink.PassivePacketSink;
import inet.networklayer.common.InterfaceTable;
import inet.networklayer.ipv4.Ipv4NetworkLayer;

network TestNetwork
{
    parameters:
        *.interfaceTableModule = absPath(".interfaceTable");
        @networkNode;
    submodules:
        interfaceTable: InterfaceTable;
        messageDispatcher: MessageDispatcher;
        ipv4: Ipv4NetworkLayer;
        upperSink: PassivePacketSink;
        lowerSink: PassivePacketSink;
        test: Test;
    connections:
        messageDispatcher.out++ --> ipv4.transportIn;
        messageDispatcher.out++ --> ipv4.ifIn;
        ipv4.transportOut --> upperSink.in;
        ipv4.ifOut --> lowerSink.in;
}

%inifile: omnetpp.ini

[General]
network = TestNetwork
ned-path = .;../../../../src
sim-time-limit = 0s

cmdenv-log-prefix = "%>"

%includes
#include "inet/common/IModuleInterfaceLookup.h"
#include "inet/common/ProtocolTag_m.h"
#include "inet/queueing/contract/IPassivePacketSink.h"

%activity:
using namespace inet;
using namespace inet::queueing;

#define CHECK(gate, argument) { std::cerr << "gate = " << #gate << ", " << printToStringIfPossible(argument, 0) << " --> " << (findModuleInterface(gate, typeid(IPassivePacketSink), &argument) != nullptr ? "found" : "not found") << std::endl; }

auto messageDispatcher = cSimulation::getActiveSimulation()->getSystemModule()->getModuleByPath("messageDispatcher");
auto transportIn = messageDispatcher->gate("out", 0);
auto ifIn = messageDispatcher->gate("out", 1);

DispatchProtocolReq dispatchProtocolReq;
dispatchProtocolReq.setProtocol(&Protocol::ipv4);

PacketProtocolTag packetProtocolTag;
packetProtocolTag.setProtocol(&Protocol::ipv4);

PacketServiceTag packetServiceTag;
packetServiceTag.setProtocol(&Protocol::ipv4);

dispatchProtocolReq.setServicePrimitive(SP_REQUEST);
CHECK(transportIn, dispatchProtocolReq);
CHECK(ifIn, dispatchProtocolReq);

dispatchProtocolReq.setServicePrimitive(SP_INDICATION);
CHECK(transportIn, dispatchProtocolReq);
CHECK(ifIn, dispatchProtocolReq);

CHECK(transportIn, packetServiceTag);
CHECK(transportIn, packetProtocolTag);

CHECK(ifIn, packetProtocolTag);
CHECK(ifIn, packetServiceTag);

%contains: stderr
gate = transportIn, DispatchProtocolReq, protocol = ipv4(38), servicePrimitive = 1 (SP_REQUEST) --> found
gate = ifIn, DispatchProtocolReq, protocol = ipv4(38), servicePrimitive = 1 (SP_REQUEST) --> not found
gate = transportIn, DispatchProtocolReq, protocol = ipv4(38), servicePrimitive = 2 (SP_INDICATION) --> not found
gate = ifIn, DispatchProtocolReq, protocol = ipv4(38), servicePrimitive = 2 (SP_INDICATION) --> found
gate = transportIn, PacketServiceTag, protocol = ipv4(38) --> found
%# nested IP packets are allowed
gate = transportIn, PacketProtocolTag, protocol = ipv4(38), frontOffset = 0 B, backOffset = 0 B --> found
gate = ifIn, PacketProtocolTag, protocol = ipv4(38), frontOffset = 0 B, backOffset = 0 B --> found
gate = ifIn, PacketServiceTag, protocol = ipv4(38) --> not found

%#--------------------------------------------------------------------------------------------------------------
%postrun-command: grep "undisposed object:" test.out > test_undisposed.out || true
%not-contains: test_undisposed.out
undisposed object: (
%#--------------------------------------------------------------------------------------------------------------
