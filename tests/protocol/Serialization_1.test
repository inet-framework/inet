%description:

Tests packet serialization.*?

%file: test.ned

import inet.queueing.source.ActivePacketSource;
import inet.queueing.sink.PassivePacketSink;
import inet.protocolelement.common.PacketSerializer;
import inet.protocolelement.common.PacketDeserializer;

network TestSerialization
{
    submodules:
        source: ActivePacketSource {
            @display("p=100,100");
        }
        serializer: PacketSerializer {
            @display("p=100,200");
        }
        deserializer: PacketDeserializer {
            @display("p=100,300");
        }
        sink: PassivePacketSink {
            @display("p=100,400");
        }
    connections:
        source.out --> serializer.in;
        serializer.out --> deserializer.in;
        deserializer.out --> sink.in;
}

%inifile: omnetpp.ini

[General]
network = TestSerialization
sim-time-limit = 10s
cmdenv-event-banners = false
*.source.packetLength = 100B
*.source.productionInterval = 1s

%subst: /\nRunning simulation\.\.\.\n//

