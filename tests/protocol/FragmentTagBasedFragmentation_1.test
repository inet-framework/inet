%description:

Tests FragmentTag based fragmentation.

%file: test.ned

import inet.queueing.source.ActivePacketSource;
import inet.queueing.sink.PassivePacketSink;
import inet.protocolelement.fragmentation.FragmentTagBasedFragmenter;
import inet.protocolelement.fragmentation.FragmentTagBasedDefragmenter;

network TestFragmentTagBasedFragmentation
{
    submodules:
        source: ActivePacketSource {
            @display("p=100,100");
        }
        fragmenter: FragmentTagBasedFragmenter {
            @display("p=100,200");
        }
        defragmenter: FragmentTagBasedDefragmenter {
            @display("p=100,300");
        }
        sink: PassivePacketSink {
            @display("p=100,400");
        }
    connections:
        source.out --> fragmenter.in;
        fragmenter.out --> defragmenter.in;
        defragmenter.out --> sink.in;
}

%inifile: omnetpp.ini

[General]
network = TestFragmentTagBasedFragmentation
sim-time-limit = 2s
cmdenv-event-banners = false
*.source.packetLength = 100B
*.source.productionInterval = 1s
*.fragmenter.fragmenterPolicy.typename = "LengthBasedFragmenterPolicy"
*.fragmenter.fragmenterPolicy.maxFragmentLength = 10B

%subst: /\nRunning simulation\.\.\.\n//

