%description:
Tests OscillatorBasedClock:

%#--------------------------------------------------------------------------------------------------------------
%inifile: omnetpp.ini
[General]
ned-path = .;../../../../src;../../lib
network = inet.test.unittest.lib.ClockTestNetwork
cmdenv-express-mode = false
cmdenv-log-prefix = "%l %C: "

*.host.clock.typename = "OscillatorBasedClock"
*.host.clock.oscillator.driftRate = 1e6 ppm
%#--------------------------------------------------------------------------------------------------------------
%postrun-command: grep INFO test.out > clock.out || true

%#--------------------------------------------------------------------------------------------------------------
%contains: clock.out
C:(ClockTestApp)ClockTestNetwork.host.app: start: simtime: 0, clock: 0, computed simtime: 0
