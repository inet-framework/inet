%description:
Tests SettableClock:

%#--------------------------------------------------------------------------------------------------------------
%inifile: omnetpp.ini
[General]
ned-path = .;../../../../src;../../lib
network = inet.test.unittest.lib.ClockTestNetwork
cmdenv-express-mode = false
cmdenv-log-prefix = "%l %C: "

*.host.clock.typename = "SettableClock"
*.host.clock.initialClockTime = 1s
*.host.clock.oscillator.driftRate = -0.5e6 ppm
*.host.clock.defaultOverdueClockEventHandlingMode = "execute"
*.scenarioManager.script = xml("<scenario><set-clock t='2.5s' module='host.clock' time='5s'/></scenario>")
%#--------------------------------------------------------------------------------------------------------------
%postrun-command: grep -E "INFO|WARN" test.out > clock.out || true
