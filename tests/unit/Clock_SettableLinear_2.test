%description:
Tests SettableClock:

%#--------------------------------------------------------------------------------------------------------------
%inifile: omnetpp.ini
[General]
ned-path = .;../../../../src;../../lib
network = inet.test.unittest.lib.ClockTestNetwork
simtime-resolution = 1fs
cmdenv-express-mode = false
cmdenv-event-banners = false
cmdenv-log-prefix = "%l %C: "
**.cmdenv-log-level = INFO

*.host.clock.typename = "SettableClock"
*.host.clock.oscillator.nominalTickLength = 1ps
*.host.clock.oscillator.driftRate = 1e6 ppm
%#--------------------------------------------------------------------------------------------------------------
%extraargs: -s
%subst: /\n<!> No more events, simulation completed.*?\n//
