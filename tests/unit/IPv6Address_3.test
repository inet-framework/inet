%description:
test IPv6Address prefix mask generation

%includes:
#include "IPv6Address.h"

%global:

%activity:
for (int i=0; i<=128; i++)
{
    uint32 mask[4];
    IPv6Address::constructMask(i, mask);
    ev << i << "  " << IPv6Address(mask[0], mask[1], mask[2], mask[3]) << "\n";
}
ev << ".\n";

